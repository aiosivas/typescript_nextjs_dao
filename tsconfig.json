  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "scripts/generatetestproposal.js", "scripts/initialize-thirdweb-sdk.js", "pages/discredirect.jsx", "pages/api/discord/add-signer-to-db.js", "utils/db.cjs"],
  "exclude": ["node_modules"]
}
